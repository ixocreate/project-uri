{
    "name": "ixocreate/project-uri-package",
    "description": "IXOCREATE URI Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/application": "dev-consolidate",
        "zendframework/zend-diactoros": "^1.7"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0",
        "vimeo/psalm": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Package\\ProjectUri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\ProjectUri\\": "tests/misc/",
            "Ixocreate\\Test\\ProjectUri\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
