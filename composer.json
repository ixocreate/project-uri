{
    "name": "ixocreate/project-uri",
    "description": "Library to manage the project URI",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/contract": "dev-develop",
        "ixocreate/config": "dev-develop",
        "zendframework/zend-diactoros": "^1.7"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0.1",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\ProjectUri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\ProjectUri\\": "tests/misc/",
            "IxocreateTest\\ProjectUri\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
