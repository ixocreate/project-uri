{
    "name": "kiwi-suite/project-uri",
    "description": "kiwi-suite/project-uri",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "kiwi-suite/contract": "dev-develop",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "kiwi-suite/application": "dev-develop",
        "zendframework/zend-diactoros": "^1.7",
        "http-interop/http-server-middleware": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "KiwiSuite\\ProjectUri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KiwiSuiteMisc\\ProjectUri\\": "tests/misc/",
            "KiwiSuiteTest\\ProjectUri\\": "tests/src/"
        }
    }
}
